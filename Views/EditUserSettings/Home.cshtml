<table class="table table-striped table-hover" >
    <thead>
        <tr>
            <th>Naam</th>
            <th>ID</th>
            <th>GCComUserID</th>
            <th>Lat</th>
            <th>Lon</th>
            <th>Geocaches op Global</th>
            <th>Land</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in @Model)
        {
            <tr>
                <td>@m.Name</td>
                <td>@m.YafUserID</td>
                <td>@m.GCComUserID</td>
                <td>@m.HomelocationLat</td>
                <td>@m.HomelocationLon</td>
                <td>@m.ShowGeocachesOnGlobal</td>
                <td>@m.DefaultCountryCode</td>
            </tr>
        }
    </tbody>
</table>
